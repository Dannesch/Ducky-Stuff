Function Set-Speaker($Volume){$wshShell = new-object -com wscript.shell;1..50 | % {$wshShell.SendKeys([char]174)};1..$Volume | % {$wshShell.SendKeys([char]175)}}
start-process -FilePath 'C:\Program Files (x86)\Google\Chrome\Application\chrome.exe' -ArgumentList 'https://www.youtube.com/watch?v=dQw4w9WgXcQ'
Set-Speaker -Volume 50
$wshell.Popup("Sponsered by CC",0,"Done",0x1)
